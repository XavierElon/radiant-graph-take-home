# Load environment variables
load('ext://dotenv', 'dotenv')
dotenv()

# Start all services using docker-compose
docker_compose('docker-compose.yml')

# Print helpful information
print('PostgreSQL is running on localhost:5433')
print('Database: radiant_graph')
print('Username: radiant')
print('Password: radiant123')
print('API is running on http://localhost:8000')
print('API Documentation is available at http://localhost:8000/docs') 